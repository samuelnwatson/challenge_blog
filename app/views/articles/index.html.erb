<h1>Challenge Blog!</h1>

<%= link_to "New Article", new_article_path %>

<table>
  <tr>
    <th>Name</th>
    <th>Content</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.name %></td>
      <td><%= article.content %></td>
      <td><%= link_to 'Show', article_path(article) %></td>
    </tr>
  <% end %>
</table>